def main():
	bool k
        int i
	def pangiann(int p1; char p2):
		
		for i := 0; i < p1; i := i + 1:
			if i mod 2 = 0:
				writeChar(p2)
				writeChar(' ')
				writeInteger(i)
				writeChar('\n')
			else:
				writeChar('f')
				writeChar('\n')
			end
		end
		p1 := 100
	end
	def bool mitsos(bool x):
		if (x = true): x := false
		else: x := true
		end
		return x
	end
	int counter
	char x
	x := 'p'
	counter := 42	
	pangiann(counter, x)
	writeString("counter = ")
	writeInteger(counter)
	writeString("\n")
	k := true
end
